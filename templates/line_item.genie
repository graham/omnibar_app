[^ expose ["content", "active", "selected", "index"] ^]

<tr>
<td class='ob-highlight'></td>

<td class='ob-checkbox-holder'>
  <input type='checkbox'
         onchange='omni_app.fire_event("control:select", {"index":[[index]]});'
         [% if selected %]checked[% end %]>
</td>

<td style='width: auto;'>
  <div class='ob-right-tags'>#asdf</div>
  <div class='ob-inner'>
    <div class='ob-litem'>[[ content ]]</div>
  </div>
</td>

</tr>

